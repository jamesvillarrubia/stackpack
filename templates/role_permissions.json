{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": "s3:*",
        "Resource": "*"
      },
      {
        "Action": [
          "s3:ListAllMyBuckets"
        ],
        "Effect": "Allow",
        "Resource": "arn:aws:s3:::*"
      },
      {
        "Action": [
          "acm:ListCertificates",
          "cloudfront:*",
          "iam:ListServerCertificates",
          "waf:ListWebACLs",
          "waf:GetWebACL"
        ],
        "Effect": "Allow",
        "Resource": "*"
      }
    ]
  }